/*每个页面公共css */
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
@font-face {
  font-family: 'iconfont';
  /* project id 1451972 */
  src: url('//at.alicdn.com/t/font_1451972_q96ctutw17.eot');
  src: url('//at.alicdn.com/t/font_1451972_q96ctutw17.eot?#iefix') format('embedded-opentype'), url('//at.alicdn.com/t/font_1451972_q96ctutw17.woff2') format('woff2'), url('//at.alicdn.com/t/font_1451972_q96ctutw17.woff') format('woff'), url('//at.alicdn.com/t/font_1451972_q96ctutw17.ttf') format('truetype'), url('//at.alicdn.com/t/font_1451972_q96ctutw17.svg#iconfont') format('svg');
}
uni-page-head .uni-page-head__title {
  font-weight: normal;
}
uni-button {
  background-color: #FFFFFF;
}
.button-hover {
  background-color: transparent;
}
.popup {
  position: fixed;
  width: 100%;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.6) none repeat scroll 0 0;
}
.disnone {
  display: none;
}
uni-radio .uni-radio-input {
  width: 30rpx;
  height: 30rpx;
}
uni-radio .uni-radio-input.uni-radio-input-checked {
  background-color: #ff6699 !important;
  border-color: #ff6699 !important;
}
uni-radio .uni-radio-input.uni-radio-input-checked:before {
  font-size: 30rpx;
}

