<view class="page"><view class="user-head"><view class="user-info"><view class="avatar"><image class="avatar-pic" src="../../static/style/avatar.png" mode="widthFix"></image></view><view class="info"><view class="title">{{''+form.cus_name+''}}<text>{{form.cus_grade_name}}</text></view><view class="phone">{{form.cus_mobile}}</view><view class="date">{{"最近到店："+form.addtime}}</view></view></view><image class="edit-icon" src="../../static/member/edit-icon.png" mode="widthFix"></image></view><view class="nav"><block wx:for="{{nav.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['navClick',[index]]]]]}}" class="{{[nav.index==index?'on':'']}}" bindtap="__e">{{item}}</text></block></view><operating hidden="{{!(nav.index==0)}}" class="page-centent" vue-id="1" form="{{form}}" bind:__l="__l"></operating><dynamic hidden="{{!(nav.index==2)}}" class="page-centent" vue-id="2" form="{{form}}" list="{{list}}" bind:__l="__l"></dynamic><distribution hidden="{{!(nav.index==3)}}" class="page-centent" vue-id="3" bind:__l="__l"></distribution><account hidden="{{!(nav.index==1)}}" class="page-centent" vue-id="4" form="{{form}}" bind:__l="__l"></account></view>